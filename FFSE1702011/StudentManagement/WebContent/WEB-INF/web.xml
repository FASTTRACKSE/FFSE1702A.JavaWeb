<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>jspServletJdbcExample</display-name>
  <context-param>
    <param-name>jdbcURL</param-name>
    <param-value>jdbc:mysql://localhost:3306/StudentManagement</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>user.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>StudentHandler</servlet-name>
    <servlet-class>com.ffse.controller.StudentHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentHandler</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>